{
  "name": "guardz-form-submission-assignment",
  "version": "1.0.0",
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "dev": "pnpm --filter ./apps/* --parallel dev",
    "backend:dev": "pnpm --filter backend dev",
    "frontend:dev": "pnpm --filter frontend dev",
    "format": "pnpm --filter ./apps/* --parallel format",
    "lint": "pnpm --filter ./apps/* --parallel lint",
    "test": "pnpm --filter ./apps/* --parallel test",
    "test:e2e": "pnpm --filter backend test:e2e"
  },
  "description": "Form submission assignment for Guardz",
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.2"
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "unrs-resolver"
    ]
  }
}
